.byt "NES",$1A,0,0,0
.byt 0,0,0,"pvtros"

VRAM	= $2000
PPUTILEM= $0200
PPUTILE = $0300
PPAL	= $3F00
JOYP	= $4016
me		= $0009
DELAY	= $000F

* = $8000
main:
.(
	ldx #0
l:
	lda #1
	sta PPUTILE,x
	inx
	beq e

	lda #2
	sta PPUTILE,x
	inx
	bne l

e:
	ldx #0
l2:
	lda img,x
	sta PPUTILE,x
	inx
	cpx #64
	bne l2

	jmp *
.)

img:
.byt $ff, $00, $ff, $00, $ff, $00, $ff, $00
.byt $ff, $00, $ff, $00, $ff, $00, $ff, $00

